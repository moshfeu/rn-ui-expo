  "bugs": {
    "url": "https://github.com/moshfeu/rn-ui-expo/issues"
  },
  "dependencies": {
    "expo": "^27.0.1",
    "react": "16.3.1",
    "react-native": "https://github.com/expo/react-native/archive/sdk-27.0.0.tar.gz"
  },
  "homepage": "https://github.com/moshfeu/rn-ui-expo#readme",
  "devDependencies": {
    "npm-auto-version": "^1.0.0",
