    "npm-publish": "cp package.json ./dist && cd ./dist && npm publish && cd ..",
    "prepublish": "npm-auto-version",
    "postpublish": "git push origin --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/moshfeu/rn-ui-expo.git"
  },
